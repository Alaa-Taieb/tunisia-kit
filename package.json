{
  "name": "tunisia-kit",
  "version": "0.2.0",
  "description": "A type-safe, zero-dependency toolkit for validating and formatting Tunisian data (CIN, phone, RIB, IBAN, currency, license plates)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./validators": {
      "types": "./dist/validators/index.d.ts",
      "import": "./dist/validators/index.js"
    },
    "./formatters": {
      "types": "./dist/formatters/index.d.ts",
      "import": "./dist/formatters/index.js"
    },
    "./data": {
      "types": "./dist/data/index.d.ts",
      "import": "./dist/data/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "tunisia",
    "tunisian",
    "validation",
    "validator",
    "formatter",
    "cin",
    "rib",
    "iban",
    "phone",
    "landline",
    "currency",
    "tnd",
    "governorates",
    "delegations",
    "license-plate",
    "matricule",
    "passport",
    "bank",
    "north-africa",
    "arabic"
  ],
  "author": {
    "name": "Alaa Taieb",
    "email": "alaataieb.tn@gmail.com",
    "url": "https://www.alaataieb.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alaa-Taieb/tunisia-kit.git"
  },
  "bugs": {
    "url": "https://github.com/Alaa-Taieb/tunisia-kit/issues"
  },
  "homepage": "https://github.com/Alaa-Taieb/tunisia-kit#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}